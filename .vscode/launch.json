    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/transformer_ppo",   // 设置工作目录
            "args": ["-Xfrozen_modules=off"]              // 关闭冻结模块
        }
    ]
}